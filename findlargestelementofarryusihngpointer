// Online C compiler to run C program online
#include <stdio.h>
int findLargest(int *arr, int n) {  
    int max = *arr;  
    for (int i = 1; i < n; i++) {  
        if (*(arr + i) > max) max = *(arr + i);  
    }  
    return max;  
}

int main() {
    int arr[] = {5,10,15,20,5};
    int n = 5;
    int b = findLargest(arr,n);
    printf("THe larget element is: %d",b);

    return 0;
}
